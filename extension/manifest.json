{
  "manifest_version": 2,

  "name": "subako",
  "version": "0.1.0",

  "description": "This extension provides fast access for esa posts.",

  "author": "Daisuke Masuyama <iacta.alea.esto@gmail.com>",

  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "history"
  ]
}
